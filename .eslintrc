{
  "extends": ["standard","plugin:react/recommended"],
  "rules": {
    "camelcase": 0,
    "eol-last": 0,
    "indent": ["error", 4, {
      "VariableDeclarator": { "let": 1, "const": 1 },
      "SwitchCase": 1
    }],
    "key-spacing": ["error", { "afterColon": true }],
    "keyword-spacing": ["error", { "overrides": { "switch": { "after": false }, "for": { "after": false }, "if": { "after": false } } }],
    "no-inner-declarations": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
    "no-useless-constructor": 0,
    "one-var": ["error", "consecutive"],
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "semi": ["error", "always"],
    "space-before-function-paren": 0
  },
  "env": {
    "browser": true
  }
}